{
  "rules": {
    "comma-dangle": [2, "always-multiline"],
  },
  "env": {
    "es6": true,
    "amd": true,
    "commonjs": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  }

}
